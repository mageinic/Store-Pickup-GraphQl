<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="Magento\StoreGraphQl\Model\Resolver\Store\StoreConfigDataProvider">
        <arguments>
            <argument name="extendedConfigData" xsi:type="array">
                <item name="store_pickup_enable"
                      xsi:type="string">store_pickup/general/enable</item>
                <item name="store_pickup_store_link_title"
                      xsi:type="string">store_pickup/general/store_link</item>
                <item name="store_pickup_store_link_location"
                      xsi:type="string">store_pickup/general/store_link_location</item>
                <item name="store_pickup_google_api"
                      xsi:type="string">store_pickup/google_api/google_map_api</item>
                <item name="store_pickup_frontend_url"
                      xsi:type="string">store_pickup/store_list_view/frontend_url</item>
                <item name="store_pickup_meta_title"
                      xsi:type="string">store_pickup/store_list_view/meta_title</item>
                <item name="store_pickup_meta_description"
                      xsi:type="string">store_pickup/store_list_view/meta_description</item>
                <item name="store_pickup_meta_keywords"
                      xsi:type="string">store_pickup/store_list_view/meta_keywords</item>
                <item name="store_pickup_default_radius"
                      xsi:type="string">store_pickup/search_config/default_radius</item>
                <item name="store_pickup_max_radius"
                      xsi:type="string">store_pickup/search_config/max_radius</item>
                <item name="store_pickup_length_option"
                      xsi:type="string">store_pickup/search_config/length_option</item>
            </argument>
        </arguments>
    </type>
</config>
